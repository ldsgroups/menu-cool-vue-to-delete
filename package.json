{
  "name": "menu-cool",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.2.0",
  "description": "Menu Cool révolutionne votre expérience au restaurant. Il suffit de scanner un QR code, de parcourir le menu interactif, de personnaliser votre commande et de déguster de délicieux plats sans attendre. C'est le futur de la commande, à portée de main",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/ldsgroups/menu-cool"
  },
  "main": "./nuxt.config.ts",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:tunnel": "nuxt dev --tunnel",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "lint:fix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "release": "release-it"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.20.0",
    "@iconify-json/radix-icons": "^1.1.14",
    "@iconify/vue": "^4.1.2",
    "@nuxt/image": "^1.7.0",
    "@nuxtjs/color-mode": "^3.4.1",
    "@nuxtjs/i18n": "^8.3.1",
    "@pinia/nuxt": "^0.5.1",
    "@unocss/eslint-config": "0.60.4",
    "@unocss/eslint-plugin": "^0.60.4",
    "@unocss/nuxt": "0.60.4",
    "@vinejs/vine": "^2.1.0",
    "@vite-pwa/nuxt": "^0.8.0",
    "@vueuse/nuxt": "10.10.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "consola": "^3.2.3",
    "eslint": "^9.4.0",
    "eslint-plugin-format": "^0.1.1",
    "knip": "^5.18.0",
    "lucide-vue-next": "^0.390.0",
    "nuxt": "^3.11.2",
    "pinia": "^2.1.7",
    "postcss-nesting": "^12.1.5",
    "radix-vue": "^1.8.3",
    "release-it": "^17.3.0",
    "shadcn-nuxt": "^0.10.4",
    "tailwind-merge": "^2.3.0",
    "unocss-preset-animations": "^1.0.2",
    "unocss-preset-shadcn": "^0.3.0",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2"
  },
  "release-it": {
    "github": {
      "release": true,
      "releaseName": "v${version}"
    },
    "git": {
      "commitMessage": "chore(release): release v${version}"
    }
  }
}
